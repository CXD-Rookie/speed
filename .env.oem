# OEM三方生产环境
# 指定环境
REACT_APP_ENV=production

# OEM生产环境 api 地址
REACT_APP_API_URL=https://cs.com
# OEM生产环境 webSocket 地址
REACT_APP_WSAPI_URL=wss://cs.com
# OEM生产环境cdn地址
REACT_APP_CDN_URL=https://cdn.accessorx.com/web/index.html

# 友盟埋点地址
REACT_APP_UM_SRC=https://v1.cnzz.com/z.js
# 三方登录网站 tid 名称
REACT_APP_TID=1
REACT_APP_TID_NAME=微信

# OEM官网连接地址
REACT_APP_WEBSITE_URL=https://.cs.com
# OEM环境官网携带渠道地址
REACT_APP_WEBSITE_CHANNEL_URL=https://.cs.com?cid=about
# 问题反馈QQ群号
REACT_APP_QQ=123456789
# 问题反馈QQ群链接地址
REACT_APP_QQ_LINk=https://qm.qq.com/

# 手机号登录验证滑块 appid
REACT_APP_CAPTCHA_APPID_LOGIN=123456789
# 口令兑换验证滑块 appid
REACT_APP_CAPTCHA_APPID_CURRENCY=123456789
# 问题反馈验证滑块 appid
REACT_APP_CAPTCHA_APPID_ISSUE=123456789

# 用户协议
REACT_APP_TERMS_ADDRESS=www.cs.com
# 隐私协议
REACT_APP_POLICY_ADDRESS=www.cs.com
# 儿童保护及监护人须知
REACT_APP_CHILDREN_ADDRESS=www.cs.com
# 自动续费协议
REACT_APP_AUTOMATIC_ADDRESS=www.cs.com

# 是否是cdn地址图片，如果是项目图片，需要进行转换 1 是cdn 0 是本地图片
REACT_APP_LOACL_IMAGE=0
# 首页logo图片地址
REACT_APP_IMAGE_LOGO=@/assets/images/common/logo.png
# 用户头像默认图片
REACT_APP_IMAGE_AVATAR_DEFAULT=@/assets/images/common/default-avatar.svg
# 用户头像未登录图片
REACT_APP_IMAGE_AVATAR_EMPTY=@/assets/images/common/no-login-avatar.svg